<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SeekBarStyle">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:focusable">true</item>
        <item name="android:progressDrawable">@drawable/seekbar_layer</item>
        <item name="android:indeterminateDrawable">@drawable/seekbar_layer</item>
        <item name="android:thumb">@drawable/seekbar_thumb_selector</item>
        <item name="android:minHeight">3dp</item>
        <item name="android:maxHeight">3dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingLeft">5dp</item>
    </style>

    <style name="Vertical_SeekBarStyle">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:focusable">true</item>
        <item name="android:progressDrawable">@drawable/volume_light_skbar_layer</item>
        <item name="android:indeterminateDrawable">@drawable/volume_light_skbar_layer</item>
        <item name="android:thumb">@drawable/seekbar_thumb_selector</item>
        <item name="android:minHeight">2dp</item>
        <item name="android:maxHeight">2dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="seekBarRotation">CW270</item>

    </style>


    <style name="controllerTimeTxtStyle">
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">@color/color_white</item>
    </style>


    <style name="volume_light_img_style">
        <item name="android:layout_height">15dp</item>
        <item name="android:layout_width">15dp</item>
        <item name="android:scaleType">centerCrop</item>
    </style>


</resources>

